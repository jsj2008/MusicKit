\pard\fs28\li2100\fc0\cf0 SYNOPSIS\

\pard\tx3120\tx3620\tx4120\b\li2620\fc0\cf0 #import <musickit/errors.h>\

\fs16 \

\pard\b0\fs28\fi-520\li3120\fc0\cf0 unsigned 
\b int MKSetTrace(
\b0 int 
