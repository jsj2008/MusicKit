\pard\tx3120\tx3620\tx4120\b\li2620\fc0\cf0 #import <musickit/Note.h>\

\fs16 \

\pard\b0\fs28\fi-520\li3120\fc0\cf0 BOOL 
\b MKIsNoteParPresent(
\b0 Note *
\i aNote
\i0 , int 
\i par
\b\i0 )\

\b0 void *
\b MKInitParameterIteration(
\b0 Note *
\i aNote
\b\i0 )\

\b0 int 
\b MKNextParameter(
\b0 Note *
\i aNote
\b\i0 , 
